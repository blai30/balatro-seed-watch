{
  "name": "balatro-seed-watch",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "start": "bun run --watch index.ts",
    "build": "bun build --compile --minify --sourcemap --bytecode index.ts --outfile dist/balatro-seed-watch",
    "build:windows": "bun build --compile --sourcemap --bytecode --target=bun-windows-x64 index.ts --outfile dist/balatro-seed-watch"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.4.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
